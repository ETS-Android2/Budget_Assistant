<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.h.horizon.budget_assistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Budget_Assistant">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".dashboard.ExpensesActivity"
            android:label="@string/expenses_title">
        </activity>
        <activity
            android:name=".dashboard.IncomesActivity"
            android:label="@string/expenses_title">
        </activity>
        <activity
            android:name=".expenses_layer.FoodActivity"
            android:label="@string/expense_title">
        </activity>
        <activity
            android:name=".expenses_layer.EducationActivity"
            android:label="@string/expense_title">
        </activity>
        <activity
            android:name=".expenses_layer.HealthActivity"
            android:label="@string/expense_title">
        </activity>
        <activity
            android:name=".expenses_layer.TravelActivity"
            android:label="@string/expense_title">
        </activity>
        <activity
            android:name=".expenses_layer.RentActivity"
            android:label="@string/expense_title">
        </activity>
        <activity
            android:name=".expenses_layer.LeisureActivity"
            android:label="@string/expense_title">
        </activity>
        <activity
            android:name=".expenses_layer.OthersActivity"
            android:label="@string/expense_title">
        </activity>
        <activity
            android:name=".expenses_layer.SubscriptionActivity"
            android:label="@string/expense_title">
        </activity>
        <activity
            android:name=".income_layer.SalaryActivity"
            android:label="@string/income_title">
        </activity>
        <activity
            android:name=".income_layer.OthersActivity"
            android:label="@string/income_title">
        </activity>
        <activity
            android:name=".income_layer.AllowanceActivity"
            android:label="@string/income_title">
        </activity>

    </application>

</manifest>